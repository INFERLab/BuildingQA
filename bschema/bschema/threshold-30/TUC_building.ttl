@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix bs: <urn:bschema#> .
@prefix ns1: <https://saref.etsi.org/saref4ener#> .
@prefix ns2: <https://brickschema.org/schema/Brick/ref#> .
@prefix ns3: <https://saref.etsi.org/core#> .
@prefix ns4: <https://saref.etsi.org/saref4bldg#> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

bs:Site_version_1 a brick:Site ;
    brick:hasPart bs:Building_version_1 ;
    ns2:hasExternalReference bs:IFCReference_version_1 .

bs:Zone_version_1 a brick:Zone ;
    brick:hasPart bs:Space_version_1 ;
    ns2:hasExternalReference bs:IFCReference_version_1 .

<urn:bschema#http://qudt.org/vocab/quantitykind/Energy_version_1> a quantitykind:Energy,
        ns3:Property .

<urn:bschema#http://qudt.org/vocab/quantitykind/Humidity_version_1> a quantitykind:Humidity,
        ns3:Property .

<urn:bschema#http://qudt.org/vocab/quantitykind/Power_version_1> a quantitykind:Power,
        ns3:Property .

<urn:bschema#http://qudt.org/vocab/quantitykind/Pressure_version_1> a quantitykind:Pressure,
        ns3:Property .

bs:Building_version_1 a brick:Building ;
    brick:hasPart bs:Storey_version_1 ;
    ns2:hasExternalReference bs:IFCReference_version_1 .

bs:Resource_version_1 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_10 ns2:ifcGlobalID rdfs:Literal ;
    ns2:ifcName rdfs:Literal .

bs:Resource_version_11 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_12 ns2:ifcGlobalID rdfs:Literal ;
    ns2:ifcName rdfs:Literal .

bs:Resource_version_13 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_14 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_15 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_2 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_3 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_4 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_5 ns2:ifcGlobalID rdfs:Literal ;
    ns2:ifcName rdfs:Literal .

bs:Resource_version_6 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_7 ns2:hasTimeseriesId rdfs:Literal .

bs:Resource_version_8 ns2:ifcGlobalID rdfs:Literal ;
    ns2:ifcName rdfs:Literal .

bs:Resource_version_9 ns2:hasTimeseriesId rdfs:Literal .

bs:Thermostat_version_2 a brick:Thermostat,
        ns4:UnitaryControlElement ;
    brick:hasPoint bs:Mode_Command_version_2 ;
    ns3:consistOf bs:Mode_Command_version_2 ;
    ns1:serialNumber rdfs:Literal .

<urn:bschema#http://qudt.org/vocab/quantitykind/Occupancy_version_1> a quantitykind:Occupancy,
        ns3:Property .

bs:Delay_Parameter_version_1 a brick:Delay_Parameter ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_11 .

bs:Lockout_Command_version_1 a brick:Lockout_Command ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_14 .

bs:Max_Air_Temperature_Setpoint_version_1 a brick:Max_Air_Temperature_Setpoint ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_1 .

bs:Min_Air_Temperature_Setpoint_version_1 a brick:Min_Air_Temperature_Setpoint ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_6 .

bs:Mode_Command_version_1 a brick:Mode_Command ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_4 .

bs:Mode_Command_version_2 a brick:Mode_Command ;
    brick:isPointOf bs:Thermostat_version_2 ;
    ns2:hasExternalReference bs:Resource_version_15 .

bs:Occupancy_Sensor_version_1 a brick:Occupancy_Sensor,
        ns3:Sensor ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_9 ;
    ns3:observes <urn:bschema#http://qudt.org/vocab/quantitykind/Occupancy_version_1> .

bs:On_Off_Command_version_1 a brick:On_Off_Command ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_7 .

bs:Run_Request_Command_version_1 a brick:Run_Request_Command ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_2 .

bs:Storey_version_1 a brick:Storey ;
    brick:hasPart bs:Space_version_1 ;
    brick:isLocationOf bs:Terminal_Unit_version_1 ;
    ns2:hasExternalReference bs:Resource_version_8 ;
    ns4:contains bs:Terminal_Unit_version_1 .

bs:Temperature_Sensor_version_1 a brick:Temperature_Sensor,
        ns3:Sensor ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_3 ;
    ns3:observes <urn:bschema#http://qudt.org/vocab/quantitykind/Temperature_version_1> .

bs:Temperature_Setpoint_version_1 a brick:Temperature_Setpoint,
        ns3:Actuator ;
    brick:isPointOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_13 ;
    ns3:controls <urn:bschema#http://qudt.org/vocab/quantitykind/Temperature_version_1> .

bs:Terminal_Unit_version_1 a brick:Terminal_Unit,
        ns4:FlowTerminal ;
    brick:hasLocation bs:Storey_version_1 ;
    ns2:hasExternalReference bs:Resource_version_10 .

<urn:bschema#http://qudt.org/vocab/quantitykind/Temperature_version_1> a quantitykind:Temperature,
        ns3:Property .

bs:IFCReference_version_1 a ns2:IFCReference ;
    ns2:ifcGlobalID rdfs:Literal ;
    ns2:ifcName rdfs:Literal .

bs:Space_version_1 a brick:Space,
        ns4:BuildingSpace ;
    brick:isLocationOf bs:Thermostat_version_1 ;
    ns2:hasExternalReference bs:Resource_version_5 ;
    ns4:contains bs:Thermostat_version_1 .

bs:Thermostat_version_1 a brick:Thermostat,
        ns4:UnitaryControlElement ;
    brick:hasLocation bs:Space_version_1 ;
    brick:hasPoint bs:Delay_Parameter_version_1,
        bs:Lockout_Command_version_1,
        bs:Max_Air_Temperature_Setpoint_version_1,
        bs:Min_Air_Temperature_Setpoint_version_1,
        bs:Mode_Command_version_1,
        bs:Occupancy_Sensor_version_1,
        bs:On_Off_Command_version_1,
        bs:Run_Request_Command_version_1,
        bs:Temperature_Sensor_version_1,
        bs:Temperature_Setpoint_version_1 ;
    ns2:hasExternalReference bs:Resource_version_12 ;
    ns3:consistOf bs:Delay_Parameter_version_1,
        bs:Lockout_Command_version_1,
        bs:Max_Air_Temperature_Setpoint_version_1,
        bs:Min_Air_Temperature_Setpoint_version_1,
        bs:Mode_Command_version_1,
        bs:Occupancy_Sensor_version_1,
        bs:On_Off_Command_version_1,
        bs:Run_Request_Command_version_1,
        bs:Temperature_Sensor_version_1,
        bs:Temperature_Setpoint_version_1 ;
    ns1:serialNumber rdfs:Literal .

